class Persona {
    //#ATRIBUTOS
    private String nombre;
    private int edad;
    private String apellido;
    private String genero;
    private String direccion;

    public Persona(String nombre, int edad, String apellido, String genero, String direccion) {
        this.nombre = nombre;
        this.edad = edad;
        this.apellido = apellido;
        this.genero = genero;
        this.direccion = direccion;
    }

    public String getNombre() {
        return nombre;
    }

    public int getEdad() {
        return edad;
    }

    public String getApellido() {
        return apellido;
    }

    public String getDireccion() {
        return direccion;
    }

    public String getGenero() {
        return genero;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public void setEdad(int edad) {
        this.edad = edad;
    }

    public void setApellido(String apellido) {
        this.apellido = apellido;
    }

    public void setDireccion(String direccion) {
        this.direccion = direccion;
    }

    public void setGenero(String genero) {
        this.genero = genero;
    }
    public void Mostrar(){
        System.out.print("NOMBRE: "+getNombre()+" ");
        System.out.print("APELLIDO: "+getApellido()+" ");
        System.out.print("EDAD: "+getEdad()+" ");
        System.out.print("GENERO: "+getGenero()+" ");
        System.out.println("DIRECCIÃ“N: "+getDireccion()+" ");
        System.out.println("");
    }
}
